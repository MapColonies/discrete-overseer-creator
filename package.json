{
  "name": "discrete-overseer-creator",
  "version": "4.4.6",
  "description": "Service that responsible for validating and ingesting discrete layers",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix",
    "prelint": "npm run format",
    "lint:openapi": "openapi lint ./openapi3.yaml ./bundledApi.yaml",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json && npm run bundleOpenApi && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./bundledApi.yaml ./dist/ && copyfiles ./package.json dist",
    "copySchema": "copyfiles -u 5 \"./node_modules/@map-colonies/mc-model-types/Schema/yaml/**/*\" ./Schema",
    "clean": "rimraf dist bundledApi.yaml",
    "install": "npx husky install",
    "pretest": "npm run bundleOpenApi",
    "bundleOpenApi": "openapi bundle --output ./bundledApi.yaml --ext yaml ./openapi3.yaml",
    "prebundleOpenApi": "npm run copySchema",
    "postbundleOpenApi": "rimraf Schema"
  },
  "directories": {
    "test": "tests"
  },
  "author": "MapColonies",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@godaddy/terminus": "^4.11.2",
    "@map-colonies/error-express-handler": "^2.1.0",
    "@map-colonies/error-types": "^1.1.5",
    "@map-colonies/express-access-log-middleware": "^2.0.1",
    "@map-colonies/js-logger": "^1.0.1",
    "@map-colonies/mc-model-types": "^15.2.0",
    "@map-colonies/mc-priority-queue": "^7.1.1",
    "@map-colonies/mc-utils": "^3.0.0",
    "@map-colonies/openapi-express-viewer": "^3.0.0",
    "@map-colonies/read-pkg": "0.0.1",
    "@map-colonies/storage-explorer-middleware": "^1.2.7",
    "@map-colonies/telemetry": "^5.3.1",
    "@opentelemetry/api": "1.4.0",
    "@opentelemetry/api-metrics": "0.24.0",
    "@opentelemetry/instrumentation-express": "0.32.1",
    "@opentelemetry/instrumentation-http": "0.35.1",
    "@placemarkio/check-geojson": "^0.1.12",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/boolean-valid": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "better-sqlite3": "^8.0.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.3",
    "gdal-async": "^3.8.1",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@map-colonies/eslint-config": "^4.0.0",
    "@map-colonies/prettier-config": "0.0.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@map-colonies/standard-version-update-openapi3-version": "0.0.2",
    "@redocly/openapi-cli": "^1.0.0-beta.94",
    "@types/better-sqlite3": "^7.6.3",
    "@types/compression": "^1.7.2",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "commitlint": "^17.4.4",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-create-mock-instance": "^2.0.0",
    "jest-html-reporters": "^3.1.4",
    "jest-openapi": "^0.14.2",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "prom-client": "^15.1.0",
    "rimraf": "^4.4.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.3.3"
  }
}
